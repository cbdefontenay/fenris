{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main-capability",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "store:default",
    "sql:default",
    "sql:allow-execute",
    "sql:allow-load",
    "sql:allow-select",
    "sql:allow-close"
  ]
}